#
# General setup
#
CONFIG_POSIX_MQUEUE=y
CONFIG_USELIB=y
CONFIG_FUTEX=y

#
# RCU Subsystem
#
# allow user to access kernel config through /proc/config.gz
CONFIG_IKCONFIG=y
CONFIG_IKCONFIG_PROC=y
CONFIG_LOG_BUF_SHIFT=16
CONFIG_MEMCG=y
CONFIG_NAMESPACES=y

#
# Kernel Performance Events And Counters
#
CONFIG_PROFILING=y

#
# Kernel Features
#
CONFIG_SCHED_MC=y
CONFIG_MCPM=y
CONFIG_NR_CPUS=2
CONFIG_PREEMPT=y
CONFIG_PREEMPT_COUNT=y
CONFIG_AEABI=y
CONFIG_HIGHMEM=y
CONFIG_FORCE_MAX_ZONEORDER=12

#
# Boot options
#
CONFIG_ZBOOT_ROM_TEXT=0x0
CONFIG_ZBOOT_ROM_BSS=0x0

#
# CPU Power Management
#
# CONFIG_CPU_FREQ_DEFAULT_GOV_PERFORMANCE is not set
CONFIG_CPU_FREQ_DEFAULT_GOV_ONDEMAND=y

#
# CPU Idle
#
CONFIG_CPU_IDLE=y
CONFIG_CPU_IDLE_MULTIPLE_DRIVERS=y
# CONFIG_CPU_IDLE_GOV_LADDER is not set
CONFIG_CPU_IDLE_GOV_MENU=y
# CONFIG_CPU_IDLE_GOV_TEO is not set
CONFIG_DT_IDLE_STATES=y

#
# Generic Driver Options
#
CONFIG_FW_LOADER_USER_HELPER_FALLBACK=y

#
# Default contiguous memory area size:
#
CONFIG_CMA_SIZE_MBYTES=128


#
# Power management options
#
CONFIG_PM_GENERIC_DOMAINS=y
CONFIG_PM_GENERIC_DOMAINS_SLEEP=y
CONFIG_PM_GENERIC_DOMAINS_OF=y

#
# Firmware Drivers
#
CONFIG_ARM_SCMI_PROTOCOL=y
CONFIG_ARM_SCMI_POWER_DOMAIN=y
CONFIG_HAVE_ARM_SMCCC=y
CONFIG_ARM_PSCI_FW=y
# CONFIG_VIRTUALIZATION is not set

#
# General architecture-dependent options
#
CONFIG_CC_HAS_STACKPROTECTOR_NONE=y
CONFIG_64BIT_TIME=y
CONFIG_REFCOUNT_FULL=y

#
# Disk-On-Chip Device Drivers
#
# CONFIG_MTD_SPI_NOR_USE_4K_SECTORS is not set

#
# LPDDR & LPDDR2 PCM memory drivers
#

#
# SCSI support type (disk, tape, CD-ROM)
#
CONFIG_CHR_DEV_SG=y

#
# MII PHY device drivers
#
CONFIG_REALTEK_PHY=y

#
# Input Device Drivers
#

#
# Touchscreen drivers
#
CONFIG_TOUCHSCREEN_EDT_FT5X06=m
CONFIG_TOUCHSCREEN_GOODIX=m

#
# Character devices
#
CONFIG_SERIAL_NONSTANDARD=y
CONFIG_LEGACY_PTY_COUNT=8
CONFIG_SERIAL_NONSTANDARD=y
CONFIG_DEVMEM=y
# CONFIG_DEVKMEM is not set

#
# Watchdog Device Drivers
#
CONFIG_WATCHDOG=y
CONFIG_WATCHDOG_NOWAYOUT=y
CONFIG_WATCHDOG_SYSFS=y

#
# CAN Device Drivers
#
CONFIG_CAN_M_CAN=y
CONFIG_CAN_M_CAN_PLATFORM=y

#
# Multiplexer I2C Chip support
#
CONFIG_I2C_SMBUS=y
CONFIG_I2C_CROS_EC_TUNNEL=m

#
# Multifunction device drivers
#
CONFIG_PROTECTION_CONSUMER=y

#
# USB HDMI CEC adapters
#
CONFIG_VIDEO_STM32_HDMI_CEC=m
CONFIG_CEC_CORE=m

#
# Camera sensor devices
#
CONFIG_VIDEO_OV5640=m
CONFIG_VIDEO_ST_MIPID02=m

#
# Graphics support
#
# to solve issue on DK2 screen
# CONFIG_DRM_FBDEV_EMULATION is not set
# CONFIG_DRM_LOAD_EDID_FIRMWARE is not set
# CONFIG_DRM_FBDEV_EMULATION is not set

#
# Sub-drivers
#

#
# Display Panels
#
CONFIG_DRM_PANEL_NOVATEK_NT35510=m
CONFIG_DRM_PANEL_ORISETECH_OTM8009A=m
CONFIG_DRM_PANEL_RAYDIUM_RM68200=m

#
# Display Interface Bridges
#
CONFIG_DRM_I2C_ADV7511=m
CONFIG_DRM_SII902X=m

#
# Frame buffer hardware drivers
#

#
# Console display driver support
#
CONFIG_DRM_STM=y
CONFIG_DRM_STM_DSI=y

#
# HD-Audio
#
CONFIG_SOUND=m
CONFIG_SND=m
CONFIG_SND_SOC=m
CONFIG_SND_SOC_STM32_SAI=m
CONFIG_SND_SOC_STM32_I2S=m
CONFIG_SND_SOC_STM32_SPDIFRX=m
CONFIG_SND_SOC_STM32_DFSDM=m
CONFIG_SND_AUDIO_GRAPH_CARD=m

#
# CODEC drivers
#
CONFIG_MFD_WM8994=m
CONFIG_SND_SOC_WM8994=m
CONFIG_SND_SOC_DMIC=m
CONFIG_SND_SOC_CS42L42=m
CONFIG_SND_SOC_CS42L51_I2C=m
CONFIG_SND_SOC_WM_HUBS=m
# CONFIG_SND_SOC_CROS_EC_CODEC is not set

#
# USB Device Class drivers
#
CONFIG_USB_ACM=m

#
# Gadget/Dual-role mode requires USB Gadget support to be enabled
#
CONFIG_USB_CONFIGFS=y

#
# USB Physical Layer drivers
#
CONFIG_TYPEC=m
CONFIG_TYPEC_STUSB=m
CONFIG_TYPEC_STUSB160X=m

#
# File systems
#
CONFIG_OVERLAY_FS=y

#
# Kernel hacking
#

#
# printk and dmesg options
#
CONFIG_DYNAMIC_DEBUG=y

#
# Compile-time checks and compiler options
#
CONFIG_DEBUG_INFO=y
CONFIG_GDB_SCRIPTS=y
CONFIG_DEBUG_SECTION_MISMATCH=y

# Security options
#
CONFIG_KEYS=y

CONFIG_CRYPTO_DEV_STM32_CRC=m
CONFIG_CRYPTO_DEV_STM32_HASH=m

#
# STM32 DFSDM
#
CONFIG_SD_ADC_MODULATOR=m
CONFIG_STM32_DFSDM_ADC=m

#
# STM32 BSEC NVMEM
#
CONFIG_NVMEM_STM32_ROMEM=y

#
# STM32 HSEM
#
CONFIG_HWSPINLOCK=y
CONFIG_HWSPINLOCK_STM32=y

#
# STM32 IPCC
#
CONFIG_STM32_IPCC=y

#
# STM32 RPROC
#
CONFIG_STM32_RPROC=m
CONFIG_RPMSG_VIRTIO=m

#
# RPMSG_TTY
#
CONFIG_RPMSG_TTY=m

#
# RPMSG client sample
#
CONFIG_SAMPLES=y
CONFIG_SAMPLE_RPMSG_CLIENT=m

#
# STM32 TIMER
#
CONFIG_MFD_STM32_TIMERS=y
CONFIG_IIO_STM32_TIMER_TRIGGER=y
CONFIG_PWM_STM32=y
CONFIG_COUNTER=y
CONFIG_STM32_TIMER_CNT=y

#
# STM32 LPTIMER
#
CONFIG_MFD_STM32_LPTIMER=m
CONFIG_PWM_STM32_LP=m
CONFIG_IIO_STM32_LPTIMER_TRIGGER=m
CONFIG_STM32_LPTIMER_CNT=m

#
# STM32 DAC
#
CONFIG_STM32_DAC=m

#
# Misc
#
CONFIG_MTD_SPI_NAND=y
CONFIG_VEXPRESS_SYSCFG=y

#
# Backlight & LCD device support
#
CONFIG_BACKLIGHT_GENERIC=y

#
# Common Clock Framework
#
CONFIG_COMMON_CLK_SCMI=y
CONFIG_COMMON_CLK_ASPEED=y

#
# Remoteproc drivers
#
CONFIG_ARM_SMC_MBOX=y
CONFIG_REMOTEPROC_SRM_CORE=y
CONFIG_REMOTEPROC_SRM_DEV=y
CONFIG_RPMSG_TTY=y

#
# DEVFREQ Drivers
#
# CONFIG_PM_DEVFREQ_EVENT is not set
CONFIG_PM_GENERIC_DOMAINS=y
CONFIG_PM_GENERIC_DOMAINS_SLEEP=y
CONFIG_PM_GENERIC_DOMAINS_OF=y


#
# Native drivers
#
# CONFIG_SENSORS_ARM_SCMI is not set

#
# Digest
#
CONFIG_CRYPTO_CRC32=m
CONFIG_CRYPTO_DEV_STM32_CRYP=m

# CONFIG_NFC is not set
# CONFIG_POWER_SUPPLY is not set
# CONFIG_CLOCK_THERMAL is not set
# CONFIG_DEVFREQ_THERMAL is not set
CONFIG_VIDEOBUF2_CORE=m
CONFIG_VIDEOBUF2_V4L2=m
CONFIG_VIDEOBUF2_MEMOPS=m
CONFIG_VIDEOBUF2_DMA_CONTIG=m
CONFIG_VIDEOBUF2_VMALLOC=m
CONFIG_VIDEO_V4L2_TPG=m
CONFIG_MEMORY=y
CONFIG_RESET_SCMI=y
CONFIG_RAS=y
# CONFIG_FSI is not set

CONFIG_JFFS2_FS=y
CONFIG_JFFS2_FS_DEBUG=0
CONFIG_JFFS2_FS_WRITEBUFFER=y
# CONFIG_JFFS2_FS_WBUF_VERIFY is not set
# CONFIG_JFFS2_SUMMARY is not set
# CONFIG_JFFS2_FS_XATTR is not set
# CONFIG_JFFS2_COMPRESSION_OPTIONS is not set
CONFIG_JFFS2_ZLIB=y
CONFIG_JFFS2_RTIME=y
CONFIG_UBIFS_FS=y
CONFIG_DEBUG_FS=y
CONFIG_OPTIMIZE_INLINING=y

CONFIG_REBOOT_MODE=y
CONFIG_SYSCON_REBOOT_MODE=y


